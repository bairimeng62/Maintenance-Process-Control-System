<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!--Background Color-->
    <Color x:Key="primaryBackColor1">#EAEAEA</Color>
    <Color x:Key="primaryBackColor2">#DBDBDB</Color>
    <Color x:Key="primaryBackColor3">#8485DD</Color>
    <Color x:Key="primaryBackColor4">#03001e</Color> 
    <Color x:Key="primaryBackColor5">#20002c</Color>
    <Color x:Key="secondaryBackColor1">#F7F7F7</Color>
    <Color x:Key="secondaryBackColor2">#F1F4FF</Color>
    <Color x:Key="secondaryBackColor3">#B9BDC5</Color>

    <!--Window.Border.Colors-->
    <Color x:Key="winBorderColor1">#6D2FFF</Color>
    <Color x:Key="winBorderColor2">#FB539B</Color>
    <Color x:Key="winBorderColor3">#836EFB</Color>

    <!--Color.Palette-->
    <SolidColorBrush x:Key="color1" Color="#9396F7"/>
    <SolidColorBrush x:Key="color2" Color="#784dfd"/>
    <SolidColorBrush x:Key="color3" Color="#4adaec"/>
    <SolidColorBrush x:Key="color4" Color="#fb539b"/>
    <SolidColorBrush x:Key="color5" Color="#7e82fc"/>
    <SolidColorBrush x:Key="color6" Color="#ffc047"/>
    <SolidColorBrush x:Key="color7" Color="#ef6c96"/>
    <SolidColorBrush x:Key="color8" Color="#5974C8"/>
    <SolidColorBrush x:Key="color9" Color="#07f3c0"/>
    <SolidColorBrush x:Key="color10" Color="#fba1aa"/>
    <SolidColorBrush x:Key="color11" Color="#0F277B"/>
    <SolidColorBrush x:Key="color12" Color="#836EFB"/>

    <!--Font.Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="#e0e1f1"/>
    <SolidColorBrush x:Key="titleColor2" Color="#090400"/>
    <SolidColorBrush x:Key="titleColor3" Color="#8A94AC"/>
    <SolidColorBrush x:Key="titleColor4" Color="#292929"/>
    <SolidColorBrush x:Key="titleColor5" Color="#082478"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#9497cd"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#7c80c2"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#A1AFD0"/>

    <!--panel.Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#F0F3FF"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#281269"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#2b1372"/>


     
</ResourceDictionary>